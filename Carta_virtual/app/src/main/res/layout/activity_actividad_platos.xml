<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_actividad_platos"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.duvangiraldo.carta_virtual.ActividadPlatos">

    <RelativeLayout
        android:id="@+id/activity_actividad_platos"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:paddingBottom="@dimen/activity_vertical_margin"
        android:paddingLeft="@dimen/activity_horizontal_margin"
        android:paddingRight="@dimen/activity_horizontal_margin"
        android:paddingTop="@dimen/activity_vertical_margin"
        tools:context="com.example.duvangiraldo.carta_virtual.ActividadPlatos">

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/activity_actividad_platos"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:paddingBottom="@dimen/activity_vertical_margin"
        android:paddingLeft="@dimen/activity_horizontal_margin"
        android:paddingRight="@dimen/activity_horizontal_margin"
        android:paddingTop="@dimen/activity_vertical_margin"
        tools:context="com.example.duvangiraldo.carta_virtual.ActividadPlatos"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true">

        <TextView
            android:text="Entradas"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:id="@+id/textView2"
            android:layout_alignParentTop="true" />

        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/cast_abc_scrubber_control_off_mtrl_alpha"
            android:id="@+id/imageButton2"
            android:layout_alignTop="@+id/imageButton"
            android:layout_toEndOf="@+id/imageButton"
            android:layout_marginStart="24dp" />

        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/cast_abc_scrubber_control_off_mtrl_alpha"
            android:id="@+id/imageButton3"
            android:layout_marginStart="32dp"
            android:layout_alignTop="@+id/imageButton2"
            android:layout_toEndOf="@+id/imageButton2" />

        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/cast_abc_scrubber_control_off_mtrl_alpha"
            android:id="@+id/imageButton4"
            android:layout_marginStart="26dp"
            android:layout_alignTop="@+id/imageButton3"
            android:layout_toEndOf="@+id/imageButton3" />

        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/cast_abc_scrubber_control_off_mtrl_alpha"
            android:id="@+id/imageButton5"
            android:layout_marginTop="65dp"
            android:layout_below="@+id/imageButton"
            android:layout_alignParentStart="true" />

        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/cast_abc_scrubber_control_off_mtrl_alpha"
            android:id="@+id/imageButton6"
            android:layout_alignTop="@+id/imageButton5"
            android:layout_alignStart="@+id/imageButton2" />

        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/cast_abc_scrubber_control_off_mtrl_alpha"
            android:id="@+id/imageButton7"
            android:layout_alignTop="@+id/imageButton6"
            android:layout_alignStart="@+id/imageButton3" />

        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/cast_abc_scrubber_control_off_mtrl_alpha"
            android:id="@+id/imageButton8"
            android:layout_alignTop="@+id/imageButton7"
            android:layout_alignStart="@+id/imageButton4" />

        <TextView
            android:text="Plato principal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_above="@+id/imageButton6"
            android:layout_alignParentStart="true"
            android:layout_marginBottom="19dp"
            android:id="@+id/textView4" />

        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/cast_abc_scrubber_control_off_mtrl_alpha"
            android:id="@+id/imageButton10"
            android:layout_alignTop="@+id/imageButton11"
            android:layout_alignStart="@+id/imageButton7" />

        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/cast_abc_scrubber_control_off_mtrl_alpha"
            android:id="@+id/imageButton11"
            android:layout_alignTop="@+id/imageButton9"
            android:layout_alignEnd="@+id/textView4" />

        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/cast_abc_scrubber_control_off_mtrl_alpha"
            android:id="@+id/imageButton12"
            android:layout_alignTop="@+id/imageButton10"
            android:layout_alignStart="@+id/imageButton8" />

        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/cast_abc_scrubber_control_off_mtrl_alpha"
            android:id="@+id/imageButton"
            android:layout_marginTop="12dp"
            android:layout_below="@+id/textView2"
            android:layout_alignParentStart="true" />

        <TextView
            android:text="Bebidas"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView6"
            android:layout_below="@+id/imageButton5"
            android:layout_alignParentStart="true"
            android:layout_marginTop="32dp" />

        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/cast_abc_scrubber_control_off_mtrl_alpha"
            android:id="@+id/imageButton9"
            android:layout_below="@+id/textView6"
            android:layout_alignParentStart="true"
            android:layout_marginTop="27dp" />

        <Button
            android:text="Ver ubicacion sede"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="20dp"
            android:id="@+id/button3"
            android:layout_alignParentBottom="true"
            android:layout_centerHorizontal="true" />

    </RelativeLayout>
</RelativeLayout>
