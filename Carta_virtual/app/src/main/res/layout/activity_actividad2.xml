<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_actividad2"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.duvangiraldo.carta_virtual.Actividad2">

    <ImageView
        android:layout_width="@dimen/cast_intro_overlay_title_margin_top"
        android:layout_height="@dimen/cast_intro_overlay_title_margin_top"
        app:srcCompat="@mipmap/res"
        android:id="@+id/imageView6"
        android:layerType="software"
        android:scaleType="fitXY"
        android:layout_marginEnd="27dp"
        android:layout_alignTop="@+id/button4"
        android:layout_alignParentEnd="true" />

    <ImageView
        android:layout_width="@dimen/cast_intro_overlay_title_margin_top"
        android:layout_height="@dimen/cast_intro_overlay_title_margin_top"
        app:srcCompat="@mipmap/res"
        android:id="@+id/imageView8"
        android:layerType="software"
        android:scaleType="fitXY"
        android:layout_alignTop="@+id/button6"
        android:layout_alignStart="@+id/imageView7" />

    <Button
        android:text="Sede 3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button6"
        android:layout_marginTop="35dp"
        android:layout_below="@+id/imageView7"
        android:layout_alignStart="@+id/button5" />

    <Button
        android:text="Sede 1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="51dp"
        android:layout_marginTop="34dp"
        android:id="@+id/button4"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true" />

    <Button
        android:text="Sede 2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button5"
        android:layout_marginTop="41dp"
        android:layout_below="@+id/imageView6"
        android:layout_alignStart="@+id/button4" />

    <ImageView
        android:layout_width="@dimen/cast_intro_overlay_title_margin_top"
        android:layout_height="@dimen/cast_intro_overlay_title_margin_top"
        app:srcCompat="@mipmap/res"
        android:id="@+id/imageView7"
        android:layerType="software"
        android:scaleType="fitXY"
        android:layout_alignTop="@+id/button5"
        android:layout_alignStart="@+id/imageView6" />

</RelativeLayout>
